#!/usr/bin/env goo
package main

import "fmt"

// Test basic func function  
func hello() {
	fmt.Println("Hello from func!")
}

// Test func with parameters
func greet(name string) {
	fmt.Println("Hello, " + name + "!")
}

// Test func with return value
func add(a, b int) int {
	return a + b
}

// Test func with multiple return values
func divmod(a, b int) (int, int) {
	return a / b, a % b
}

// Test func main function
func main() {
	hello()
	greet("World")
	
	result := add(5, 3)
	fmt.Printf("5 + 3 = %d\n", result)
	
	quotient, remainder := divmod(17, 5)
	fmt.Printf("17 / 5 = %d remainder %d\n", quotient, remainder)
	
	// Test anonymous func function
	square := func(x int) int {
		return x * x
	}
	
	fmt.Printf("square(4) = %d\n", square(4))
}
